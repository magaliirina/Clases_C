#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int numeroUno;
	int numeroDos;
	int suma;
	printf("Ingrese el primer numero:");
	printf("Ingrese el segundo numero:");
	scanf("%d",&numeroUno);
	scanf("%d",&numeroDos);
	suma=numeroUno+numeroDos;
	printf("La suma es:&d\n",suma);

	return EXIT_SUCCESS;
}
