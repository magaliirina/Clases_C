#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int numeroUno;
	int numeroDos;
	int suma;
	printf("Ingrese el primer numero: %d",numeroUno);
	printf("Ingrese el segundo numero: %d",numeroDos);
	scanf("%d",&numeroUno);
	scanf("%d",&numeroDos);
	suma=numeroUno+numeroDos;
}
