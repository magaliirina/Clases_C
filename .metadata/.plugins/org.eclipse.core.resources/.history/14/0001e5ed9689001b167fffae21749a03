#include <stdio.h>
#include <stdlib.h>

void muestra(int x,int y);

int main(void) {
	int x,y;

	printf("Ingrese un numero");
	scanf("%d",&x);
	printf("Ingrese un numero");
	scanf("%d",&y);
	muestra(x,y);
	printf("\nxvale %d \ny vale %d",x,y);
	setbuf(stdout,NULL);
	return EXIT_SUCCESS;
}

void muestra(int x,int y);{
	x=y;

}
