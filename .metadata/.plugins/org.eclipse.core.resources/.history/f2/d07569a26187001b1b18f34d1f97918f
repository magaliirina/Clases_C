#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i;
	int i=27;
	float pi=3.14;
	char c='A';
	printf("Hola mundo");
	//(%tipo de variable,nombre de variable)
	printf("numeros enteros: %d",i);
	printf("numeros con coma: %f",pi);
	printf("numeros enteros: %c",c);



	return EXIT_SUCCESS;
}
